<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>CollectDataServlet</servlet-name>
        <servlet-class>crawl.CollectDataServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>IPManipulatorrServlet</servlet-name>
        <servlet-class>crawl.IPManipulatorrServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>IPManipulatorServlet</servlet-name>
        <servlet-class>crawl.IPManipulatorServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CollectDataServlet</servlet-name>
        <url-pattern>/CollectDataServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>IPManipulatorrServlet</servlet-name>
        <url-pattern>/IPManipulatorrServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>IPManipulatorServlet</servlet-name>
        <url-pattern>/IPManipulatorServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>CollectDataServlet</welcome-file>
    </welcome-file-list>
</web-app>
